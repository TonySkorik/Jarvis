{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Jarvis.Server": {
      "commandName": "Project",
      "commandLineArgs": "--config-path=\"./\"",
      "launchBrowser": true,
      "launchUrl": "api/status",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:5007"
    },
    "Docker": {
      "commandName": "Docker",
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/status",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}